{
  "name": "my-little-spiders",
  "version": "1.0.0",
  "description": "Express app for improving your relationships with spiders",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "nodemon index.js",
    "start": "node index.js",
    "tailwind:css": "postcss ./public/tailwind.css -o ./public/styles.css --watch",
    "test": "mocha --recursive",
    "coverage": "nyc --all mocha --recursive",
    "coverage:report": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xShipi/my-little-spiders.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "ejs",
    "tailwindcss"
  ],
  "author": "Sipos Áron Dávid <sipisipos@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xShipi/my-little-spiders/issues"
  },
  "homepage": "https://github.com/xShipi/my-little-spiders#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "mongoose": "6.x"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.22",
    "nyc": "^15.1.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "tailwindcss": "^3.3.1"
  }
